{
  "name": "feature",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/feature/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tools/feature",
        "main": "tools/feature/src/index.ts",
        "tsConfig": "tools/feature/tsconfig.lib.json",
        "assets": [
          "tools/feature/*.md",
          {
            "input": "./tools/feature/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./tools/feature/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./tools/feature",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./tools/feature",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "publish": {
      "executor": "dcat23-nx:publish",
      "options": {
        "distPath": "dist/tools/feature"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "tools/feature/jest.config.ts"
      }
    }
  }
}
